{
  "name": "react-redux-jest-kit",
  "version": "1.0.5",
  "description": "StarterKit with react, redux, eslint, webpack and jest.",
  "main": "index.js",
  "scripts": {
    "test": "jest -u",
    "test-watch": "jest -u --watch",
    "start": "node ./config/server",
    "build": "rimraf dist && NODE_ENV=production webpack --config ./config/webpack.prod.js --progress --profile --colors",
    "eslint": "eslint ."
  },
  "jest": {
    "bail": true,
    "verbose": true,
    "collectCoverage": true,
    "notify": true
  },
  "keywords": [
    "es6",
    "react",
    "redux",
    "webpack",
    "hmr",
    "jest"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/trembacz/react-redux-jest-kit.git"
  },
  "author": "Tomasz Rembacz (trembacz@squiz.pl)",
  "license": "MIT",
  "dependencies": {
    "file-loader": "^1.1.5",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "3.1.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "eslint": "^4.11.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "json-loader": "^0.5.7",
    "node-sass": "^4.7.2",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "^2.0.8",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.1.1",
    "redux-mock-store": "^1.3.0",
    "rimraf": "^2.6.2",
