{
  "name": "react-redux-jest-kit",
  "version": "1.0.2",
  "description": "StarterKit with react, redux, eslint, webpack and jest.",
  "main": "index.js",
  "scripts": {
    "test": "jest -u",
    "test-watch": "jest -u --watch",
    "start": "node ./config/server",
    "build": "rimraf dist && NODE_ENV=production webpack --config ./config/webpack.prod.js --progress --profile --colors",
    "eslint": "eslint ."
  },
  "jest": {
    "bail": true,
    "verbose": true,
    "collectCoverage": true,
    "notify": true
  },
  "keywords": [
    "es6",
    "react",
    "redux",
    "webpack",
    "hmr",
    "jest"
	],
	"repository": {
    "type": "git",
    "url": "https://github.com/trembacz/react-redux-jest-kit.git"
	},
  "author": "Tomasz Rembacz (trembacz@squiz.pl)",
  "license": "MIT",
  "dependencies": {
    "file-loader": "^0.11.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.2.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^20.0.4",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.3",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
